mylist = ["haha","hehe","heihei"]
index = mylist.index("haha")
print(index)
mylist[2] = "hengheng"
print(mylist)
mylist.insert(1,"hoho") # insert插入
print(mylist)
mylist.append("huhu")   # append追加
print(mylist)
mylist.extend(["hihi","huihui","hunhun"]) # extend加容器
print(mylist)
del mylist[0]
print(mylist)
quchu = mylist.pop(0)
print(mylist)
print(quchu)
